{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --open",
    "build:json": "node ./CICD/build_json.mjs",
    "build": "pnpm build:json && vite build && cp ./dist/index.html ./dist/404.html",
    "build:github": "pnpm build:json && vite build --base=/blue-archive/ && cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build:json && vite preview --host",
    "check": "eslint --fix ./src/ ./CICD/ ./public/config/yaml/ --ext .js,.ts,.vue,.yml,.yaml && vue-tsc --noEmit --skipLibCheck",
    "up": "taze major -I",
    "create-story": "node ./CICD/create-story/create-favor.mjs"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.5.13",
    "@vueuse/core": "^12.4.0",
    "axios": "^1.8.2",
    "ba-story-player": "workspace:*",
    "fastest-levenshtein": "^1.0.16",
    "gsap": "^3.12.5",
    "husky": "^9.1.7",
    "ismobilejs": "^1.1.1",
    "mitt": "~3.0.1",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.2.0",
    "pinyin-pro": "^3.26.0",
    "typed.js": "^2.1.0",
    "vue": "^3.5.13",
    "vue-router": "~4.5.0",
    "xxhashjs": "~0.2.2",
    "autoprefixer": "~10.4.20",
    "dayjs": "~1.11.13",
    "element-plus": "^2.9.3",
    "radash": "~12.1.0",
    "vite-plugin-vue-devtools": "^7.7.0",
    "unocss": "^65.4.0",
    "globals": "^15.0.0"
  },
  "devDependencies": {
    "taze": "^18.1.0",
    "@babel/preset-env": "^7.26.0",
    "@csstools/css-parser-algorithms": "^3.0.3",
    "@csstools/css-tokenizer": "^3.0.3",
    "@types/node": "^22.10.5",
    "@types/toastify-js": "^1.12.3",
    "@types/xxhashjs": "~0.2.4",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "chalk": "^5.4.1",
    "eslint": "9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sort-exports": "^0.9.1",
    "eslint-plugin-vue": "^9.32.0",
    "eslint-plugin-yaml": "^1.0.3",
    "js-yaml": "^4.1.0",
    "postcss": "~8.4.49",
    "postcss-plugin-px2rem": "^0.8.1",
    "postcss-preset-env": "^10.1.3",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.83.1",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "vite": "^5.4.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.21.1",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.0",
    "workbox-window": "^7.3.0",
    "ba-rig": "workspace:*"
  }
}
